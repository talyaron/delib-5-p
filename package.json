{
	"name": "delib-5",
	"private": true,
	"version": "1.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite --mode development",
		"dev:h": "vite --host",
		"devf": "cd functions && npm run build:watch",
		"deve": "firebase emulators:start",
		"build": "tsc && vite build",
		"lint": "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\"",
		"typecheck": "tsc",
		"preview": "vite preview",
		"test:unit": "vitest",
		"check-all": "npm run lint && tsc && vite build --mode development",
		"deploy:host-prod": "tsc && vite build --mode production && firebase deploy --only hosting",
		"deploy:host-dev": "tsc && vite build --mode development && firebase deploy --only hosting",
		"deploy:f": "cd functions && npm run build && firebase deploy --only functions",
		"deploy": "npm run deploy:host-prod && npm run deploy:f"
	},
	"dependencies": {
		"@dagrejs/dagre": "^1.0.4",
		"@reduxjs/toolkit": "^1.9.5",
		"delib-npm": "^1.3.15",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-sonarjs": "^1.0.3",
		"firebase": "^10.0.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-redux": "^8.1.1",
		"react-router-dom": "^6.22.3",
		"reactflow": "^11.11.1",
		"rollup-plugin-visualizer": "^5.10.0",
		"sass": "^1.63.6",
		"xlsx": "^0.18.5",
		"zod": "^3.21.4"
	},
	"devDependencies": {
		"@types/react": "^18.2.14",
		"@types/react-dom": "^18.2.6",
		"@typescript-eslint/eslint-plugin": "^5.61.0",
		"@typescript-eslint/parser": "^5.61.0",
		"@vitejs/plugin-react-swc": "^3.3.2",
		"cypress": "^13.6.4",
		"depcheck": "^1.4.7",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^8.7.0",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-react": "^7.33.2",
		"prettier": "^3.1.1",
		"typescript": "^5.3.3",
		"vite": "^5.1.3",
		"vite-plugin-pwa": "^0.19.0",
		"vite-plugin-svgr": "^4.2.0",
		"vitest": "^1.6.0",
		"workbox-window": "^7.0.0"
	}
}
